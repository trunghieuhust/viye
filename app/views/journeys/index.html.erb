<h1>Listing journeys</h1>

<table>
  <thead>
    <tr>
      <th>Startdate</th>
      <th>Enddate</th>
      <th>Content</th>
      <th>Headline</th>
      <th>Media</th>
      <th>Mediacaption</th>
      <th>Mediacredit</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @journeys.each do |journey| %>
      <tr>
        <td><%= journey.startdate %></td>
        <td><%= journey.enddate %></td>
        <td><%= journey.content %></td>
        <td><%= journey.headline %></td>
        <td><%= journey.media %></td>
        <td><%= journey.mediacaption %></td>
        <td><%= journey.mediacredit %></td>
        <td><%= link_to 'Show', journey %></td>
        <td><%= link_to 'Edit', edit_journey_path(journey) %></td>
        <td><%= link_to 'Destroy', journey, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Journey', new_journey_path %>
